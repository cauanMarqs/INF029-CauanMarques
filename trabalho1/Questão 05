int q5(int num) {
    
	int invertido = 0;

    while (num != 0) {
        int digito = num % 10; 
        invertido = invertido * 10 + digito; 
        num /= 10;
    }
    
	num = invertido;
    return num;
}

int main (){

    printf("%d\n", q5(345) == 543);
    printf("%d\n", q5(78) == 87);
    printf("%d\n", q5(3) == 3);
    printf("%d\n", q5(5430) == 345);
}

